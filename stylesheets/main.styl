@import url http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,700italic,300,400,700

body
 background #f1f4f5
 color #000
 font-family open-sans


#formLogin
	background #fff
	padding 15px
	border 1px solid #eaedee
	border-radius 4px
h1
	text-align center
#formLogin label
	font-size 16px
#formLogin .checkbox
	width 40%
	float left
	margin-top 0
#formLogin .checkbox-ri
	width 40%
	float right
	margin-top 0
	text-align right
#formLogin .btn-login
	width 100%
	color #000
	font-size 20px
	opacity .87
	height 45px

#formLogin a
	color #ff9800
	opacity .87

#formLogin p
	font-size 16px
	text-align center

#formRegister h2
	font-size 20px
	padding 30px
	width 100%
	text-align center
	margin 0

#formRegister
	background #fff
	padding 15px
	border 1px solid #eaedee
	border-radius 4px

#v label
	font-size 16px
#formRegister .checkbox
	float left
	margin-top 0
#formRegister .checkbox-ri
	width 40%
	float right
	margin-top 0
	text-align right
#formRegister .btn-login
	width 100%
	color #000
	font-size 20px
	opacity .87
	height 45px

#formRegister a
	color #ff9800
	opacity .87

#formRegister p
	font-size 16px
	text-align center

.title-bottom
	padding-top 20px	

.header
	height 50px
	background #000
	padding 0 
	box-shadow 0 0 5px
.header-left
	border-right 1px solid #fff
	height 100%
	padding 5px 0 0 15px
.navbar-header a
	font-size 16px
	padding 5px 0 5px 45px
	line-height 45px
	color #fff

.navbar-header a.selected
	color #fff
	opacity .30

.container-fluid 
	padding 0

.header .col-lg-2 .row .col-lg-4 img
	border-radius 50%

.header .col-lg-2 .row .col-lg-8
	height 50px
	text-align left
	padding 0
.header .col-lg-2 .row .col-lg-8 ul
	height 100%
	padding 10px
	float left !important

.header .row .navbar-nav ul
	height 100%


.header .row .navbar-nav ul a
	padding 20px

.header .row .navbar-nav a.dropdown-toggle
	color #fff
	font-size 16px

.footer
	position absolute
	height 50px
	background #000 
	opacity .87
	bottom 0
	width 100%
.footer p
	font-size 14px
	color #fff
	padding 0 10px
	margin 0
	line-height 49px

.container
	width 100%
	padding 0

.container .row-bck
	width 100%
	height 70px
	background #69a125
	padding 0
	margin 0

.bootstrap-select 
	padding 15px 0 0 0
	width 80% !important
	height 40px !important

.button-plus
	background #ff9800
	float right
	position absolute
	top 15px
	left 82%
	border-radius 2px

.button-plus span
	font-size 20px
	padding 7px

.container .row .col-lg-4 
	height 100%	

.header-table
	border 1px solid #d8dbdc
	padding 10px 20px
	margin 20px 20px 0 20px
	background-color #e5e5e5

.header-table .col-lg-1 span
	line-height 50px
.header-table .col-lg-1 .input-group
	width 35% !important
	float left
	padding 5px 0

.header-table .bootstrap-select
	padding 0
	border-radius 2px
	height auto !important
.header-table .bootstrap-select button
	border-radius 3px !important
	width 100%
.header-table .input-group
	width 100%

.header-table input[type="radio"], input[type="checkbox"] 
    line-height normal
    margin 5px 0 0
.glyphicon-search
	color #908f8f

.header-table .table-responsive
	margin-top 0 !important

.header-table-bottom
	border 0px solid #d8dbdc
	padding 0px
	margin 0 20px 20px 20px
	background-color #e5e5e5

.header-table-bottom .col-lg-1 span
	line-height 50px
.header-table-bottom .col-lg-1 .input-group
	width 35% !important
	float left
	padding 5px 0

.header-table-bottom .bootstrap-select
	padding 0
	border-radius 2px
	height auto !important
.header-table-bottom .bootstrap-select button
	border-radius 3px !important
	width 100%
.header-table-bottom .input-group
	width 100%

.header-table-bottom input[type="radio"], input[type="checkbox"] 
    line-height normal
    margin 5px 0 0
.glyphicon-search
	color #908f8f

.header-table-bottom .table-responsive
	margin-top 0 !important

table.dataTable thead th, table.dataTable thead td 
    border-bottom 0 solid #111
    padding 3px 18px
table.dataTable tbody th, table.dataTable tbody td 
    padding 2px 10px








	
